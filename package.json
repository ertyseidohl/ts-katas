    "test:002": "tsc --strict --outDir dist test/002-types/002-types.test.ts && node --test dist/002-types.test.js",
    "test:002:solution": "tsc --strict --outDir dist solutions/002-types/002-types.test.ts && node --test dist/002-types.test.js"
